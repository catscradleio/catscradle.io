  "scripts": {
    "backend": "nodemon --exec babel-node backend/server.js",
    "frontend": "npm start --prefix frontend",
    "postinstall": "cd frontend && npm install",
    "server": "concurrently \"npm run backend\" \"npm run frontend\"",
