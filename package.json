{
  "name": "catscradle.io",
  "version": "1.0.0",
  "description": "Play pictionary!",
  "main": "index.js",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/node": "^7.2.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
